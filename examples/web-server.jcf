# Real-world Example: Web Server Configuration
# Demonstrates: nested structures and conditionals

env = "production"
is_production = true

server = (
  host = "0.0.0.0",
  port = 443,
  ssl = true
)

database = (
  type = "postgres",
  host = "db.example.com",
  port = 5432
)

app = (
  name = "MyApp",
  server = server,
  database = database
)
